<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.cxengine.android"
        android:versionCode="1"
        android:versionName="1.0">

    <uses-sdk  android:targetSdkVersion="19"  android:minSdkVersion="9" />
    
    <uses-feature android:glEsVersion="0x00020000" android:required="true"/>
    
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>
    
    <application
        android:icon="@drawable/appicon" 
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">>
        <activity android:name="com.cxengine.EngineActivity"
                android:label="@string/app_name"
                android:screenOrientation="landscape"
                android:configChanges="orientation|keyboardHidden">
            <meta-data android:name="android.app.lib_name" android:value="cxEngineAndroid" />
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest> 
