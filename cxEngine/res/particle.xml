<?xml version="1.0" encoding="UTF-8"?>

<!--TextureKey at *.xml file use-->
<cxViewRoot cxView.fill="true">

    <cxParticle
        cxSprite.texture="n.png"
        cxView.cropping="false"
        cxView.w = "200"
        cxView.h = "100"
        cxParticle.type="cxEmitterType('gravity')"
        cxParticle.number = "2"
        cxParticle.blend = "cxBlendMode('multiple')"
        cxParticle.duration = "-1"
        cxParticle.life = "1.5"
        cxParticle.speed = "200,200"
        cxParticle.angle = "90,45"
        cxParticle.gravity = "0,-100"
        cxParticle.todir = "true"
        cxParticle.startspin="1000"
        cxParticle.startsize = "60"
        cxParticle.endsize = "60"
        cxParticle.startcolor="1,1,1,1"
        cxParticle.endcolor="1,1,1,0"
        cxParticle.rate = "-1" />

    <cxParticle cxParticle.pex = "particle.pex" />
    <cxButton
        cxView.w = "180"
        cxView.h = "120"
        cxView.right="10"
        cxView.bottom="10"
        cxButton.onRelease="cxPopView">
        <cxAtlas cxView.fill="0,0,0,0" cxAtlas.scale9="50,50,50,50" cxSprite.texture="scale9.png" />
        <cxLabelTTF cxView.color="0,0,0" cxLabelTTF.font="banana.ttf" cxLabelTTF.text="Return" cxLabelTTF.size="30"/>
    </cxButton>
</cxViewRoot>
