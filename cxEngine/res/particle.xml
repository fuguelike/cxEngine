<?xml version="1.0" encoding="UTF-8"?>

<!--TextureKey at *.xml file use-->
<cxViewXML cxView.fill="true">
    
    <cxParticle
        cxSprite.texture="n.png"
        cxView.cropping="false"
        cxView.w = "200"
        cxView.h = "100"
        cxParticle.number = "2"
        cxParticle.blend = "multiple"
        cxParticle.duration = "-1"
        cxParticle.life = "1.5"
        cxParticle.speed = "200,200"
        cxParticle.angle = "90,45"
        cxParticle.gravity = "0,-100"
        cxParticle.todir = "true"
        cxParticle.startspin="1000"
        cxParticle.startsize = "60"
        cxParticle.endsize = "60"
        cxParticle.startcolor="1,1,1,1"
        cxParticle.endcolor="1,1,1,0"
        cxParticle.rate = "-1" />
    
    <cxParticle
        cxSprite.cached="false"
        cxSprite.texture = "w.png"
        cxView.cropping = "false"
        cxView.w = "200"
        cxView.h = "100"
        cxParticle.number = "30"
        cxParticle.blend = "multiple"
        cxParticle.gravity = "0,-200"
        cxParticle.duration = "-1"
        cxParticle.life = "1.5"
        cxParticle.speed = "200,200"
        cxParticle.angle = "0,360"
        cxParticle.todir = "true"
        cxParticle.startspin = "0,1000"
        cxParticle.startsize = "10,10"
        cxParticle.endsize = "10,10"
        cxParticle.startcolor = "1,1,1,1"
        cxParticle.endcolor = "1,1,1,0"
        cxParticle.rate = "-1" />
    
    <cxButton
        cxView.w = "180"
        cxView.h = "120"
        cxView.right="10"
        cxView.bottom="10"
        cxButton.onRelease="cxPopView">
        <cxAtlas cxView.fill="true" cxAtlas.scale9="50,50,50,50" cxSprite.texture="scale9.png" />
        <cxLabel cxView.color="0,0,0" cxLabel.font="banana.ttf" cxLabel.text="Return" cxLabel.size="30"/>
    </cxButton>
    
</cxViewXML>
